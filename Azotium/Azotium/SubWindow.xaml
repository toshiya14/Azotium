<Window x:Name="window" x:Class="Azotium.SubWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Azotium"
        xmlns:ctrls="clr-namespace:Azotium.Controls"
        mc:Ignorable="d"
        Title="SubWindow"
        Topmost="True"
        ResizeMode="CanResizeWithGrip"
        WindowStartupLocation="Manual"
        WindowStyle="None"
        AllowsTransparency="True"
        Loaded="window_Loaded"
        Background="#19000000"
        d:DataContext="{Binding Sub, Source={StaticResource Locator}}"
        Top="{Binding Y, Mode=TwoWay}"
        Left="{Binding X, Mode=TwoWay}"
        Width="{Binding Width, Mode=TwoWay}"
        Height="{Binding Height, Mode=TwoWay}"
        ShowInTaskbar="False">
    <Grid>
        <ctrls:WebBrowser x:Name="wb" DataContext="{Binding DataContext, ElementName=window}" />
    </Grid>


</Window>
